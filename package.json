{
  "name": "ticket-genie",
  "scripts": {
    "postinstall": "npx patch-package",
    "build": "npx tsup ./src/main.ts",
    "dev": "npm run build -- --onSuccess=\"NODE_ENV=development node dist/main.cjs\" --watch",
    "start": "node dist/main.cjs",
    "test": "dotenv -e .env.test -- npx jest --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "type": "module",
  "devDependencies": {
    "@swc/core": "^1.3.40",
    "@types/node": "^18.15.3",
    "discordx": "^11.7.1",
    "dotenv": "^16.0.3",
    "reflect-metadata": "^0.1.13",
    "signales": "^2.0.5",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "zod": "^3.21.4"
  }
}
